{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run test.ps1",
      "type": "PowerShell",
      "request": "launch",
      "script": "${workspaceFolder}/test.ps1",
      "cwd": "${workspaceFolder}",
      "createTemporaryIntegratedConsole": true
    },
    {
      "name": "Run test.js",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/test.js",
      "cwd": "${workspaceFolder}",
      "console": "internalConsole"
    },
    {
      "name": "Run powershell/test.ps1",
      "type": "PowerShell",
      "request": "launch",
      "script": "${workspaceFolder}/powershell/test.ps1",
      "cwd": "${workspaceFolder}/powershell",
      "createTemporaryIntegratedConsole": true
    },
    {
      "name": "Run node/test.js",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node/test.js",
      "cwd": "${workspaceFolder}/node",
      "console": "internalConsole"
    },
    {
      "name": "Run node/server.js",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node/server.js",
      "cwd": "${workspaceFolder}/node",
      "console": "internalConsole"
    }
  ]
}
