{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run test.js",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/test.js",
      "cwd": "${workspaceFolder}",
      "console": "internalConsole"
    },
    {
      "name": "Run test.js with preLaunchTask",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/test.js",
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "preLaunchTask": "sleep-build-delay"
    }
  ]
}
